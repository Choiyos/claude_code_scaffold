# Optimized for Claude Code Development Environment

# Temporarily change options.
'builtin' 'local' '-a' 'p10k_config_opts'
[[ ! -o 'aliases'         ]] || p10k_config_opts+=('aliases')
[[ ! -o 'sh_glob'         ]] || p10k_config_opts+=('sh_glob')
[[ ! -o 'no_brace_expand' ]] || p10k_config_opts+=('no_brace_expand')
'builtin' 'setopt' 'no_aliases' 'no_sh_glob' 'brace_expand'

() {
  emulate -L zsh -o extended_glob

  # Unset all configuration options.
  unset -m '(POWERLEVEL9K_*|DEFAULT_USER)~POWERLEVEL9K_GITSTATUS_DIR'

  # Zsh >= 5.1 is required.
  autoload -Uz is-at-least && is-at-least 5.1 || return

  # Prompt colors.
  local grey='242'
  local red='1'
  local yellow='3'
  local blue='4'
  local magenta='5'
  local cyan='6'
  local white='7'

  # Left prompt segments.
  typeset -g POWERLEVEL9K_LEFT_PROMPT_ELEMENTS=(
    os_icon                 # os identifier
    dir                     # current directory
    vcs                     # git status
    context                 # user@host
    command_execution_time  # previous command duration
    virtualenv              # python virtual environment
    pyenv                   # python environment
    nodeenv                 # node.js environment
    node_version            # node.js version
    go_version              # go version
    rust_version            # rust version
    docker_context          # docker context
    kubecontext             # current kubernetes context
    terraform               # terraform status
    aws                     # aws profile
    azure                   # azure account
    gcloud                  # google cloud project
    newline                 # \n
    prompt_char             # prompt symbol
  )

  # Right prompt segments.
  typeset -g POWERLEVEL9K_RIGHT_PROMPT_ELEMENTS=(
    status                  # exit code of the last command
    command_execution_time  # previous command duration
    background_jobs         # presence of background jobs
    direnv                  # direnv status
    asdf                    # asdf version manager
    virtualenv              # python virtual environment
    anaconda                # conda environment
    pyenv                   # python environment
    goenv                   # go environment
    nodenv                  # node.js environment
    nvm                     # node version manager
    nodeenv                 # node environment
    node_version            # node.js version
    package                 # package version
    rbenv                   # ruby environment
    rvm                     # ruby version manager
    fvm                     # flutter version manager
    luaenv                  # lua environment
    jenv                    # java environment
    plenv                   # perl environment
    phpenv                  # php environment
    scalaenv                # scala environment
    haskell_stack           # haskell stack
    kubecontext             # current kubernetes context
    terraform               # terraform workspace
    aws                     # aws profile
    aws_eb_env              # aws elastic beanstalk environment
    azure                   # azure account
    gcloud                  # google cloud project
    google_app_cred         # google application credentials
    context                 # user@hostname
    nordvpn                 # nordvpn connection status
    ranger                  # ranger shell
    nnn                     # nnn shell
    vim_shell               # vim shell indicator
    midnight_commander      # midnight commander shell
    nix_shell               # nix shell
    disk_usage              # disk usage
    ram                     # free RAM
    swap                    # used swap
    load                    # CPU load
    time                    # current time
  )

  # Basic style options.
  typeset -g POWERLEVEL9K_BACKGROUND=                            # transparent background
  typeset -g POWERLEVEL9K_{LEFT,RIGHT}_{LEFT,RIGHT}_WHITESPACE=  # no surrounding whitespace
  typeset -g POWERLEVEL9K_{LEFT,RIGHT}_SUBSEGMENT_SEPARATOR=' '  # separate segments with a space
  typeset -g POWERLEVEL9K_{LEFT,RIGHT}_SEGMENT_SEPARATOR=        # no end-of-line symbol

  # Disable segment icons by default.
  typeset -g POWERLEVEL9K_VISUAL_IDENTIFIER_EXPANSION=

  # Add an empty line before each prompt.
  typeset -g POWERLEVEL9K_PROMPT_ADD_NEWLINE=true

  # Connect left prompt lines with these symbols.
  typeset -g POWERLEVEL9K_MULTILINE_FIRST_PROMPT_PREFIX='%F{blue}‚ï≠‚îÄ'
  typeset -g POWERLEVEL9K_MULTILINE_NEWLINE_PROMPT_PREFIX='%F{blue}‚îú‚îÄ'
  typeset -g POWERLEVEL9K_MULTILINE_LAST_PROMPT_PREFIX='%F{blue}‚ï∞‚îÄ'

  # Prompt symbol configuration.
  typeset -g POWERLEVEL9K_PROMPT_CHAR_OK_{VIINS,VICMD,VIVIS,VIOWR}_FOREGROUND=$green
  typeset -g POWERLEVEL9K_PROMPT_CHAR_ERROR_{VIINS,VICMD,VIVIS,VIOWR}_FOREGROUND=$red
  typeset -g POWERLEVEL9K_PROMPT_CHAR_{OK,ERROR}_VIINS_CONTENT_EXPANSION='‚ùØ'
  typeset -g POWERLEVEL9K_PROMPT_CHAR_{OK,ERROR}_VICMD_CONTENT_EXPANSION='‚ùÆ'
  typeset -g POWERLEVEL9K_PROMPT_CHAR_{OK,ERROR}_VIVIS_CONTENT_EXPANSION='V'
  typeset -g POWERLEVEL9K_PROMPT_CHAR_{OK,ERROR}_VIOWR_CONTENT_EXPANSION='‚ñ∂'
  typeset -g POWERLEVEL9K_PROMPT_CHAR_OVERWRITE_STATE=true
  typeset -g POWERLEVEL9K_PROMPT_CHAR_LEFT_PROMPT_LAST_SEGMENT_END_SYMBOL=' '

  # Current directory configuration.
  typeset -g POWERLEVEL9K_DIR_FOREGROUND=$blue
  typeset -g POWERLEVEL9K_SHORTEN_STRATEGY=truncate_to_unique
  typeset -g POWERLEVEL9K_SHORTEN_DELIMITER=
  typeset -g POWERLEVEL9K_DIR_SHORTENED_FOREGROUND=$grey
  typeset -g POWERLEVEL9K_DIR_ANCHOR_FOREGROUND=$blue
  typeset -g POWERLEVEL9K_DIR_ANCHOR_BOLD=true
  typeset -g POWERLEVEL9K_SHORTEN_FOLDER_MARKER='.git'
  typeset -g POWERLEVEL9K_DIR_MAX_LENGTH=80
  typeset -g POWERLEVEL9K_DIR_HYPERLINK=true

  # Git status configuration.
  typeset -g POWERLEVEL9K_VCS_CLEAN_FOREGROUND=$green
  typeset -g POWERLEVEL9K_VCS_UNTRACKED_FOREGROUND=$yellow
  typeset -g POWERLEVEL9K_VCS_MODIFIED_FOREGROUND=$yellow
  typeset -g POWERLEVEL9K_VCS_BRANCH_ICON='\uF126 '
  typeset -g POWERLEVEL9K_VCS_GIT_GITHUB_ICON='\uF408 '
  typeset -g POWERLEVEL9K_VCS_GIT_BITBUCKET_ICON='\uF171 '
  typeset -g POWERLEVEL9K_VCS_GIT_GITLAB_ICON='\uF296 '
  typeset -g POWERLEVEL9K_VCS_GIT_ICON='\uF1D3 '

  # Context configuration (user@hostname).
  typeset -g POWERLEVEL9K_CONTEXT_FOREGROUND=$yellow
  typeset -g POWERLEVEL9K_CONTEXT_ROOT_FOREGROUND=$red
  typeset -g POWERLEVEL9K_CONTEXT_{REMOTE,REMOTE_SUDO}_FOREGROUND=$magenta
  typeset -g POWERLEVEL9K_CONTEXT_TEMPLATE='%n@%m'

  # Python virtual environment.
  typeset -g POWERLEVEL9K_VIRTUALENV_FOREGROUND=$cyan
  typeset -g POWERLEVEL9K_VIRTUALENV_SHOW_PYTHON_VERSION=true
  typeset -g POWERLEVEL9K_VIRTUALENV_{LEFT,RIGHT}_DELIMITER=

  # Node version.
  typeset -g POWERLEVEL9K_NODE_VERSION_FOREGROUND=$green
  typeset -g POWERLEVEL9K_NODE_VERSION_PROJECT_ONLY=true

  # Go version.
  typeset -g POWERLEVEL9K_GO_VERSION_FOREGROUND=$cyan
  typeset -g POWERLEVEL9K_GO_VERSION_PROJECT_ONLY=true

  # Rust version.
  typeset -g POWERLEVEL9K_RUST_VERSION_FOREGROUND=$yellow
  typeset -g POWERLEVEL9K_RUST_VERSION_PROJECT_ONLY=true

  # Docker context.
  typeset -g POWERLEVEL9K_DOCKER_CONTEXT_FOREGROUND=$blue
  typeset -g POWERLEVEL9K_DOCKER_ICON='üê≥'

  # Kubernetes context.
  typeset -g POWERLEVEL9K_KUBECONTEXT_FOREGROUND=$magenta
  typeset -g POWERLEVEL9K_KUBECONTEXT_DEFAULT_FOREGROUND=$magenta
  typeset -g POWERLEVEL9K_KUBECONTEXT_SHOW_ON_COMMAND='kubectl|helm|kubens|kubectx|k9s'

  # Terraform workspace.
  typeset -g POWERLEVEL9K_TERRAFORM_FOREGROUND=$blue
  typeset -g POWERLEVEL9K_TERRAFORM_SHOW_ON_COMMAND='terraform|tf'

  # AWS profile.
  typeset -g POWERLEVEL9K_AWS_FOREGROUND=$yellow
  typeset -g POWERLEVEL9K_AWS_SHOW_ON_COMMAND='aws|awscli'

  # Azure account.
  typeset -g POWERLEVEL9K_AZURE_FOREGROUND=$blue
  typeset -g POWERLEVEL9K_AZURE_SHOW_ON_COMMAND='az|azure'

  # Google Cloud project.
  typeset -g POWERLEVEL9K_GCLOUD_FOREGROUND=$blue
  typeset -g POWERLEVEL9K_GCLOUD_SHOW_ON_COMMAND='gcloud|gsutil'

  # Command execution time.
  typeset -g POWERLEVEL9K_COMMAND_EXECUTION_TIME_FOREGROUND=$yellow
  typeset -g POWERLEVEL9K_COMMAND_EXECUTION_TIME_THRESHOLD=3
  typeset -g POWERLEVEL9K_COMMAND_EXECUTION_TIME_PRECISION=0
  typeset -g POWERLEVEL9K_COMMAND_EXECUTION_TIME_FORMAT='d h m s'

  # Background jobs.
  typeset -g POWERLEVEL9K_BACKGROUND_JOBS_FOREGROUND=$cyan
  typeset -g POWERLEVEL9K_BACKGROUND_JOBS_VERBOSE=false

  # Time.
  typeset -g POWERLEVEL9K_TIME_FOREGROUND=$grey
  typeset -g POWERLEVEL9K_TIME_FORMAT='%D{%H:%M:%S}'
  typeset -g POWERLEVEL9K_TIME_UPDATE_ON_COMMAND=false

  # System load.
  typeset -g POWERLEVEL9K_LOAD_WHICH=5
  typeset -g POWERLEVEL9K_LOAD_NORMAL_FOREGROUND=$green
  typeset -g POWERLEVEL9K_LOAD_WARNING_FOREGROUND=$yellow
  typeset -g POWERLEVEL9K_LOAD_CRITICAL_FOREGROUND=$red

  # Disk usage.
  typeset -g POWERLEVEL9K_DISK_USAGE_NORMAL_FOREGROUND=$green
  typeset -g POWERLEVEL9K_DISK_USAGE_WARNING_FOREGROUND=$yellow
  typeset -g POWERLEVEL9K_DISK_USAGE_CRITICAL_FOREGROUND=$red
  typeset -g POWERLEVEL9K_DISK_USAGE_WARNING_LEVEL=90
  typeset -g POWERLEVEL9K_DISK_USAGE_CRITICAL_LEVEL=95

  # RAM.
  typeset -g POWERLEVEL9K_RAM_FOREGROUND=$yellow

  # OS icon.
  typeset -g POWERLEVEL9K_OS_ICON_FOREGROUND=$white
  typeset -g POWERLEVEL9K_OS_ICON_CONTENT_EXPANSION='${P9K_CONTENT}'

  # Instant prompt mode.
  typeset -g POWERLEVEL9K_INSTANT_PROMPT=verbose

  # Hot reload.
  (( ! $+functions[p10k] )) || p10k reload
}

# Tell `p10k configure` which file it should overwrite.
typeset -g POWERLEVEL9K_CONFIG_FILE=${${(%):-%x}:a}

(( ${#p10k_config_opts} )) && setopt ${p10k_config_opts[@]}
'builtin' 'unset' 'p10k_config_opts'